{
  "name": "React-Template-App",
  "version": "0.0.1",
  "description": "A robust template app for building SPA's",
  "main": "index.js",
  "author": "Charles Garrett",
  "license": "MIT",
  "engines": {
    "node": "6.3.1",
    "npm": "3.10.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charleslavon/reactjs-spring-app-template.git"
  },
  "dependencies": {
    "axios": "^0.15.2",
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-eslint": "^7.0.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.9.0",
    "bower": "1.8.0",
    "browser-sync": "2.13.0",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.0",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "node-sass": "^3.10.1",
    "npm-run-all": "2.3.0",
    "path": "^0.12.7",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-fb-auth": "^1.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "uuid": "^2.0.3",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.5.1",
    "eslint": "^3.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "1.5.5",
    "eslint-plugin-react": "^6.4.1",
    "eslint-watch": "^2.1.14",
    "jquery": "^3.1.1",
    "mocha": "^3.1.2",
    "react-addons-test-utils": "^15.3.2",
    "sinon": "^1.17.6"
  },
  "options": {
    "mocha": "tools/testSetup.js app/**/*.spec.js --reporter progress"
  },
  "scripts": {
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:webapp": "babel-node tools/webappServer.js",
    "lint": "esw webpack.config.* src tools --color --ignore-pattern node_modules; exit 0;",
    "lint:watch": "esw webpack.config.* src tools --color --ignore-pattern node_modules --watch",
    "clean-webapp": "npm run remove-webapp && mkdir webapp",
    "remove-webapp": "rimraf ./webapp",
    "prebuild": "npm run clean-webapp && npm run lint && npm run test",
    "build": "babel-node tools/build.js",
    "buildAndRun": "babel-node tools/build.js && npm run open:webapp",
    "testonly": "./node_modules/.bin/_mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly",
    "test:watch": "npm run test -- --watch"
  }
}
